{
  "name": "gatsby-plugin-gatsby-cloud",
  "description": "A Gatsby plugin which optimizes working with Gatsby Cloud",
  "version": "4.0.0-zz-next.1",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "date-fns": "^2.23.0",
    "fs-extra": "^10.0.0",
    "gatsby-core-utils": "^3.0.0-zz-next.1",
    "gatsby-telemetry": "^3.0.0-zz-next.1",
    "kebab-hash": "^0.1.2",
    "lodash": "^4.17.21",
    "webpack-assets-manifest": "^5.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.2.1",
    "babel-preset-gatsby-package": "^2.0.0-zz-next.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "jest": "^27.2.1",
    "msw": "^0.35.0",
    "node-fetch": "^3.0.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-gatsby-cloud#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "http/2-server-push",
    "gatsby-cloud"
  ],
  "types": "./index.d.ts",
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^4.0.0-next",
    "webpack": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-gatsby-cloud"
  },
  "sideEffects": false,
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\" && npm run clean && npm run copy-type-declarations",
    "clean": "del-cli ./components/index.d.ts",
    "copy-type-declarations": "cpy src/components/index.d.ts components/",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
